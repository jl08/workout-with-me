<h4>Please Login Here</h4>

<%= form_for :session, url: '/login', html: {id: "login"} do |f| %>
  
  <%= f.text_field :email, placeholder: "Email" %>
  <div class="spacer"></div>

  <%= f.password_field :password, placeholder: "Password" %>
  <div class="spacer"></div>
  <%= f.submit "Login" %>
<% end %>